{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/WebApiTemplate/WebApiTemplate.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish (FDD)",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
				"-c",
				"Release",
                "${workspaceFolder}/src/WebApiTemplate/WebApiTemplate.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish (SCD [win-x64])",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
				"-c",
				"Release",
				"-r",
				"win-x64",
				"--self-contained",
				"true",
                "${workspaceFolder}/src/WebApiTemplate/WebApiTemplate.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish (SCD [linux-x64])",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
				"-c",
				"Release",
				"-r",
				"linux-x64",
				"--self-contained",
				"true",
                "${workspaceFolder}/src/WebApiTemplate/WebApiTemplate.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}